  "scripts": {
    "build:dev": "NODE_ENV=development yarn clean && gatsby build && yarn copy:static",
    "build": "NODE_ENV=production yarn clean && yarn clean:cache && yarn copy:talks && gatsby build --log-pages && yarn copy:static",
    "build:no-clean": "gatsby build && yarn copy:static",
    "build:no-cache": "yarn clean && yarn clean:cache && gatsby build && yarn copy:static",
    "clean": "rimraf ./public",
    "clean:cache": "rimraf ./.cache",
    "copy:static": "node ./build-scripts/copy-static.js",
    "copy:talks": "node ./build-scripts/copy-talks.js",
    "dev": "gatsby develop",
    "lint": "eslint src/**/**/*.tsx",
    "lint:fix": "eslint --fix src/**/*/*.tsx",
    "start": "yarn dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
